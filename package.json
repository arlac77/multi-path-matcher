{
  "name": "multi-path-matcher",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/multi-path-matcher.mjs",
  "exports": {
    ".": "./src/multi-path-matcher.mjs"
  },
  "description": "finds and decodes best matching path in a set of routes",
  "keywords": [
    "router",
    "routing"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/multi-path-matcher.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/multi-path-matcher.mjs"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "^13.0.2",
    "semantic-release": "^17.2.1"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/multi-path-matcher.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/multi-path-matcher/issues"
  },
  "homepage": "https://github.com/arlac77/multi-path-matcher#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
